CREATE TABLE `RDPLogins` (
	`STOREID`	INTEGER NOT NULL,
	`STORECODE`	TEXT NOT NULL,
	`AUTHCODE`	TEXT NOT NULL,
	`TILLNUMBER`	INTEGER,
	`RDPAddress`	TEXT,
	`RDPPort`	INTEGER,
	`RDPLogin`	TEXT,
	`RDPPassword`	TEXT,
	PRIMARY KEY(`STOREID`)
);

CREATE TABLE `Registration` (
	`ID`	INTEGER PRIMARY KEY AUTOINCREMENT,
	`DATE`	TEXT NOT NULL,
	`STORECODE`	TEXT NOT NULL,
	`TILLNUMBER`	INTEGER,
	`EXTERNALIPADDRESS`	TEXT,
	`INTERNALIPADDRESS`	TEXT
);
